{
  "name": "cra-electron-typescript-forge",
  "version": "0.1.0",
  "private": true,
  "author": "Ronald",
  "description": "cra-electron-typescript-forge",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "node-pre-gyp": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1"
  },
  "main": "./src/electron.js",
  "scripts": {
    "dev": "concurrently -k \"BROWSER=NONE react-scripts start\" \"wait-on tcp:3000 && electron-forge start\"",
    "start": "HTTPS=true react-scripts start",
    "build": "HTTPS=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/ramda": "^0.27.40",
    "concurrently": "^6.0.0",
    "electron": "^12.0.2",
    "electron-devtools-installer": "^3.1.1",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "ramda": "^0.27.1",
    "wait-on": "^5.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "public/icons/mac/icon.icns",
        "name": "CRA + electron + typescript + forge"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cra_electron_typescript_forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
